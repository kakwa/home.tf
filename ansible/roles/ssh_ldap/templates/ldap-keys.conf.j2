# LDAP connection for ssh-ldap-keys (AuthorizedKeysCommand)
# Managed by Ansible - do not edit by hand

LDAP_URI="{{ ssh_ldap_uri }}"
LDAP_BASE="{{ ssh_ldap_user_ou }}"
LDAP_GROUP_BASE="{{ ssh_ldap_group_ou }}"
LDAP_BIND_DN="{{ ssh_ldap_bind_dn }}"
LDAP_BIND_PWFILE="{{ ssh_ldap_authorized_keys_pwfile }}"
LDAP_CACERT="{{ ssh_ldap_tls_cacert }}"
LDAP_REQCERT="{{ ssh_ldap_tls_reqcert }}"
